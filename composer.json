{
    "name": "kodilab/laravel-batuta",
    "description": "Laravel permissions",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jordi",
            "email": "jordi@kodilab.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Kodilab\\LaravelBatuta\\": "src",
            "Kodilab\\LaravelBatuta\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kodilab\\LaravelBatuta\\LaravelBatutaProvider"
            ],
            "aliases": {
                "Batuta": "Kodilab\\LaravelBatuta\\Facades\\Batuta"
            }
        }
    }
}
